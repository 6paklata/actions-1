{
  "name": "keybase-notifications",
  "version": "0.0.0",
  "private": true,
  "description": "Trigger Keybase chat notifications from GitHub actions",
  "scripts": {
    "preinstall": "./scripts/install_keybase_binary.sh",
    "build": "ncc build src/index.ts",
    "test": "jest --silent",
    "clean": "rm -rf  node_modules yarn-error.log dist",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint:eslint": "eslint \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lintfix": "yarn run lintfix:prettier && yarn run lintfix:eslint",
    "lintfix:prettier": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lintfix:eslint": "eslint --fix \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "dependencies": {
    "@actions/core": "^1.0.0",
    "@actions/github": "^1.1.0",
    "@types/lodash": "^4.14.139",
    "keybase-bot": "^3.0.2"
  },
  "devDependencies": {},
  "main": "index.js",
  "license": "MIT"
}
